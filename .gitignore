# ================================
# Laravel Default Ignore
# ================================
/vendor/
/node_modules/

.env
.env.*
.phpunit.result.cache
.phpactor.json

/public/hot
/public/storage
/storage/*.key
/storage/pail

/public/build
/.phpunit.cache

# ================================
# System Files
# ================================
.DS_Store
Thumbs.db
.fleet
.idea
.vscode
.zed

auth.json
Homestead.json
Homestead.yaml

# ================================
# LOGS
# ================================
*.log
storage/logs/*
bootstrap/cache/*.php

# ================================
# DO NOT UPLOAD TXT FILES
# ================================
*.txt

# ================================
# Backup Files
# ================================
*.bak
*.backup
*.old
*.tmp

# ================================
# Public Storage (Do not upload)
# ================================
public/storage/*
!public/storage/.gitignore   # اترك ملف gitignore داخلها فقط

# ================================
# Cache
# ================================
.cache/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
